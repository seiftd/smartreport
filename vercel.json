{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/server.js"
    }
  ],
  "installCommand": "cd backend && npm install",
  "buildCommand": "cd backend && echo 'Backend build completed'",
  "outputDirectory": "backend"
}
